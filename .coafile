Default.enabled = true
CPPCheck.enabled = true
LineCounting.enabled = false

[Default]
files = Source/*.cpp, Source/*/*.cpp, Source/Makefile

[CPPCheck]
bears = KeywordBear, SpaceConsistencyBear, LineLengthBear
cs_keywords = 
ci_keywords = \#TODO, \# TODO, \#FIXME, \# FIXME
use_spaces = false
max_line_length = 120

[LineCounting]
bears = LineCountBear